{
  "contributions": [
    {
      "id": "hello-world-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "Sample hub group",
      "targets": ["ms.vss-web.project-hub-groups-collection"],
      "properties": {
        "name": "Hello World!",
        "icon": {
          "dark": "img/world.png",
          "light": "img/world.png"
        }
      }
    },
    {
      "id": "default-hub",
      "type": "ms.vss-web.hub",
      "targets": [".hello-world-hub-group"],
      "properties": {
        "name": "hub.html!",
        "uri": "dist/hub/hub.html"
      }
    },
    {
      "id": "test_second_page-hub",
      "type": "ms.vss-web.hub",
      "targets": [".hello-world-hub-group"],
      "properties": {
        "name": "Direct second page",
        "uri": "dist/hub/main"
      }
    }
  ]
}
